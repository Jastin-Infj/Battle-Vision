{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "ESNext",
    "target": "ES2022",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "esnext"
    ],
    "jsx": "preserve",
    "allowJs": true,
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    // "noEmit": true,
    "incremental": true,
    "isolatedModules": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "noEmit": true
  },
  "include": [
    "src/**/**/*.ts",
    ".next/types/**/*.ts",
    "app/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
